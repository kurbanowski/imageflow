# FastAPI Photo Sharing App - Environment Configuration
# Copy this file to .env and fill in your values

# Application Environment
ENVIRONMENT=production

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=photos

# S3 Configuration for file storage
S3_BUCKET_NAME=photo-host-918801323166-9222025

# Authentication (Optional - for development with Replit Auth)
REPL_ID=your_repl_id_here

# Session Secret (Generate a secure random string)
SESSION_SECRET=your_session_secret_here